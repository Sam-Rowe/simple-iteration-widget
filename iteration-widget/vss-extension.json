{
    "manifestVersion": 1,
    "id": "iteration-widget",
    "version": "1.0.4",
    "name": "Iteration widget",
    "description": "Widget to show iteration details",
    "publisher": "SamuelRowe",
    "categories": ["Azure Boards"],
    "targets": [
      {
        "id": "Microsoft.VisualStudio.Services"
      }
    ],
    "icons": {
      "default": "img/logo.png"
    },
    "contributions": [
      {
        "id": "IterationWidget",
        "type": "ms.vss-dashboards-web.widget",
        "targets": ["ms.vss-dashboards-web.widget-catalog"],
        "properties": {
          "name": "Iteration Widget",
          "description": "My first widget",
          "catalogIconUrl": "img/CatalogIcon.png",
          "previewImageUrl": "img/preview.png",
          "uri": "iteration.html",
          "supportedSizes": [
            {
              "rowSpan": 1,
              "columnSpan": 2
            }
          ],
          "supportedScopes": ["project_team"]
        }
      }
    ],
    "files": [
      {
        "path": "iteration.html",
        "addressable": true
      },
      {
        "path": "sdk/scripts",
        "addressable": true
      },
      {
        "path": "img",
        "addressable": true
      }
    ],
    "scopes":[
      "vso.work"
    ]
  } 